{
  "name": "free-web-icons",
  "displayName": "web_icons",
  "description": "free web icons",
  "version": "0.0.1",
  "publisher": "rahulor",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "web, icons"
  ],
  "activationEvents": [
    "onCommand:icons.showMessage",
    "onView:icons-sidebar"
  ],
  "main": "dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "icons-sidebar-view",
          "title": "icons",
          "icon": "media/smiley.svg"
        }
      ]
    },
    "views": {
      "icons-sidebar-view": [
        {
          "type": "webview",
          "id": "icons-sidebar",
          "name": "icons",
          "icon": "media/smiley.svg",
          "contextualTitle": "icons"
        }
      ]
    },
    "commands": [
      {
        "command": "icons.showMessage",
        "category": "simple icons",
        "title": "Hello World"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rahul0070050/free-web-icons"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "package": "rm -rf dist  && rm -rf out && rollup -c && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^4.0.1",
    "svelte-check": "^3.4.4",
    "svelte-icon": "^2.0.0",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}